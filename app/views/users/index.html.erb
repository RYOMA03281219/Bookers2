<body>
  <main>
    <div class="body">
      <div class="container px-5 px-sm-0">
        <div class="row">
          <div class="col-md-3">
            <%= render 'users/info', user: current_user %>
            <%= render 'books/info', book: @book %>
          </div>
          <div class="col-md-8 offset-md-1">
            <h2>Users</h2>
              <table class="table table-hover table-inverse">
                <thead>
                  <tr>
                  <th>image</th>
                  <th>name</th>
                  <th colspan="3"></th>
                </tr>
              </thead>
              <tbody></tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </main>
</body>

